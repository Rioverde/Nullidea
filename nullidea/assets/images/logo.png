    <template id="commentTemplate">
    <div class="comment">
        <div class="avatarFrame commentAuthorImageFrame noSelect">
            <img width="36" height="36" class="commentAuthorImage">
        </div>
        <div class="commentData">
            <h4 class="noSelect">
                <span class="commentAuthorName">Anonymous</span>
                <a class="commentLink">
                    <time-ago format="micro" class="commentCreated"></time>
                </a>
                <span class="commentButtons">
                    <button type="button" class="dotResolveButton">Resolve</button>
                    <button type="button" class="dotReopenButton">Reopen</button>
                    <button type="button" class="commentEditButton">Edit</button>
                    <button type="button" class="commentDeleteButton">Delete</button>
                </span>
            </h4>
            <div class="commentArea">
                <div class="commentWrapper">
                    <div class="commentMirror">&nbsp;</div>
                    <textarea placeholder="Type something" maxlength="1024" class="commentNote"></textarea>
                </div>
                <button type="button" class="commentDoneButton"></button>
            </div>
            <relative-time class="commentEdited"></time>
        </div>
    </div>
</template>
<div id="dotPopup" class="hidden">
    <div id="dotPopupAnchor"></div>
    <div id="dotColors">
        <div class="dotColor yellow" data-color="yellow"></div>
        <div class="dotColor orange" data-color="orange"></div>
        <div class="dotColor peach" data-color="peach"></div>
        <div class="dotColor green" data-color="green"></div>
        <div class="dotColor turquoise" data-color="turquoise"></div>
        <div class="dotColor cornflowerBlue" data-color="cornflowerBlue"></div>
        <div class="dotColor deepPurple" data-color="deepPurple"></div>
    </div>
    <div id="dotComments"></div>
    <div id="newComment">
        <div id="newCommentButtons">
            <div id="newCommentReactions">
                <button type="button" class="newCommentReaction" data-reaction="positive">
                    <img src="/img/icReactionPositive.png" srcset="/img/icReactionPositive@2x.png 2x, /img/icReactionPositive@3x.png 3x">
                </button>
                <button type="button" class="newCommentReaction" data-reaction="neutral">
                    <img src="/img/icReactionNeutral.png" srcset="/img/icReactionNeutral@2x.png 2x, /img/icReactionNeutral@3x.png 3x">
                </button>
                <button type="button" class="newCommentReaction" data-reaction="negative">
                    <img src="/img/icReactionNegative.png" srcset="/img/icReactionNegative@2x.png 2x, /img/icReactionNegative@3x.png 3x">
                </button>
            </div>
            <button type="button" id="newCommentButton">POST</button>
        </div>
        <div id="newCommentArea">
            <div id="newCommentWrapper">
                <div id="newCommentMirror">&nbsp;</div>
                <textarea placeholder="Type something" maxlength="1024" id="newCommentNote"></textarea>
            </div>
            <button type="button" id="newCommentSendButton"></button>
        </div>
    </div>
</div>

    <dialog id="androidResFolderReminderDialog">
    <div class="androidResFolderReminderDialogHeader">
        <h1>Export assets to your project’s <strong>res</strong> folder and Zeplin will remember the location!</h1>
    </div>
    <form method="dialog">
        <img src="/img/icAndroidAssetExport.png"
             srcset="/img/icAndroidAssetExport@2x.png 2x, /img/icAndroidAssetExport@3x.png 3x">
        <div class="buttonContainer">
            <button class="done" id="androidResFolderReminderDoneButton">
                GOT IT
            </button>
        </div>
    </form>
</dialog>
    <dialog id="deleteBarrelDialog">
    <h2>Enter the name of the <span class="barrelType"></span> to confirm:</h2>
    <form method="dialog">
        <div class="inputWrapper barrelName" title="Name">
            <div class="inputArea">
                <input type="text" name="barrelName" placeholder="Name" id="barrelNameInput" maxlength="100" required autocomplete="off">
                <div class="inputUnderline"></div>
            </div>
        </div>
        <div class="buttonContainer">
            <button type="button" class="cancel" id="deleteBarrelDialogCancelButton">
                CANCEL
            </button>
            <button type="button" class="done" id="deleteBarrelDialogDoneButton" disabled>
                DELETE
            </button>
        </div>
    </form>
</dialog>

